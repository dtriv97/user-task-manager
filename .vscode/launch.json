{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Task Manager",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "dotnet",
      "args": ["watch", "run", "--project", "${workspaceFolder}/TaskManager"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
